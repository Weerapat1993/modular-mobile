{
  "plugins": [
    "plugins/markdown"
  ],
  "recurseDepth": 10,
  "source": {
      "include": [
        "README.md",
        "src/utils/ArrayCollection",
        "src/utils/Calculator",
        "src/utils/modular/Reducer"
      ],
      "exclude": [
        "src/utils/modular/Reducer/Model.js",
        "src/utils/modular/Reducer/TestReducer.js"
      ],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": true
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/docdash"
  }
}