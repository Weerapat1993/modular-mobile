{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
      "include": [
        "src/utils/Calculator",
        "src/utils/modular/Reducer"
      ],
      "exclude": [
        "src/utils/modular/Reducer/Model.js",
        "src/utils/modular/Reducer/TestReducer.js",
        "src/utils/modular/Reducer/BaseReducer.js"
      ],
